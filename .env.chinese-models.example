# 向量数据库配置 Chinese-Optimized Vector Databases
# ========================================

# 星环Milvus Configuration
MILVUS_HOST=localhost
MILVUS_PORT=19530
MILVUS_COLLECTION=langchain_learning

# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=langchain_learning

# Chroma Configuration
CHROMA_HOST=localhost
CHROMA_PORT=8000
CHROMA_COLLECTION=langchain_learning

# Weaviate Configuration
WEAVIATE_URL=http://localhost:8080
WEAVIATE_COLLECTION=LangChainLearning

# ========================================
# 应用配置 Application Settings
# ========================================
DEBUG=true
LOG_LEVEL=INFO

# 默认模型选择和建议配置
DEFAULT_PROVIDER=deepseek  # deepseek, zhipu, moonshot, openai, azure
DEFAULT_MODEL=deepseek-chat
DEFAULT_EMBEDDING_PROVIDER=zhipu  # zhipu, dashscope, openai
DEFAULT_EMBEDDING_MODEL=embedding-2

# API速率限制和重试配置
MAX_RETRIES=3
REQUEST_TIMEOUT=60
RATE_LIMIT_DELAY=1.0

# 多语言支持
LANGUAGE=zh-CN  # zh-CN, en-US, ja-JP, ko-KR
ENABLE_CHINESE_OPTIMIZATION=true

# Docker和容器化配置
DOCKER_COMPOSE_FILE=docker-compose.yml
API_PORT=8000
WEB_PORT=3000

# 中文优化配置
ENABLE_JIEBA_TOKENIZE=true
ENABLE_TRADITIONAL_CHINESE=false
FORCE_UTF8_ENCODING=true